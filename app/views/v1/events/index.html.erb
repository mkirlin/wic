<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Photo URL</th>
      <th>Date</th>
      <th>Ticket URL</th>
      <th>About</th>
      <th>Time</th>
      <th>Location</th>
      <th>Address</th>
      <th>Private Event</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= event.photo_url %></td>
        <td><%= event.date %></td>
        <td><%= event.ticket_link %></td>
        <td><%= event.about %></td>
        <td><%= event.time %></td>
        <td><%= event.location %></td>
        <td><%= event.address %></td>
        <td><%= event.is_private %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
